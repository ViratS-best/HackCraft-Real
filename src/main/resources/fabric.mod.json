{
	"schemaVersion": 1,
	"id": "soulswap",
	"version": "${version}",
	"name": "Soul Swap",
	"description": "A combat/utility mod for tactical repositioning! Throw an Ender Pearl at a mob to swap positions. Hostile mobs take confusion damage, passive animals grant Invisibility.",
	"authors": [
		"Virat"
	],
	"contact": {},
	"license": "MIT",
	"icon": "assets/soulswap/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.soulswap.SoulSwapMod"
		],
		"client": [
			"com.soulswap.SoulSwapClientMod"
		]
	},
	"mixins": [
		"soulswap.mixins.json",
		{
			"config": "soulswap.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.2",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}